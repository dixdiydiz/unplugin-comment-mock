{
	"name": "unplugin-comment-mock",
	"type": "module",
	"version": "1.0.0",
	"description": "mock data through comment",
	"license": "MIT",
	"homepage": "https://github.com/dixdiydiz/unplugin-comment-mock#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dixdiydiz/unplugin-comment-mock.git"
	},
	"bugs": {
		"url": "https://github.com/dixdiydiz/unplugin-comment-mock/issues"
	},
	"keywords": [
		"mock",
		"comment",
		"unplugin",
		"vite",
		"webpack",
		"rollup",
		"transform"
	],
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./rspack": {
			"import": "./dist/rspack.js",
			"require": "./dist/rspack.cjs"
		},
		"./vite": {
			"import": "./dist/vite.js",
			"require": "./dist/vite.cjs"
		},
		"./webpack": {
			"import": "./dist/webpack.js",
			"require": "./dist/webpack.cjs"
		},
		"./rollup": {
			"import": "./dist/rollup.js",
			"require": "./dist/rollup.cjs"
		},
		"./esbuild": {
			"import": "./dist/esbuild.js",
			"require": "./dist/esbuild.cjs"
		},
		"./farm": {
			"import": "./dist/farm.js",
			"require": "./dist/farm.cjs"
		},
		"./types": {
			"import": "./dist/types.js",
			"require": "./dist/types.cjs"
		},
		"./utils": {
			"import": "./dist/utils.js",
			"require": "./dist/utils.cjs"
		},
		"./*": "./*"
	},
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"typesVersions": {
		"*": {
			"*": ["./dist/*", "./*"]
		}
	},
	"files": ["dist"],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch src",
		"build:fix": "esno scripts/postbuild.ts",
		"lint": "biome check src",
		"play": "pnpm -C playground run dev",
		"prepublishOnly": "pnpm run build",
		"release": "bumpp && pnpm publish",
		"start": "esno src/index.ts",
		"test": "vitest"
	},
	"engines": {
		"node": ">=22"
	},
	"packageManager": "pnpm@10.6.4",
	"peerDependencies": {
		"@farmfe/core": ">=1",
		"esbuild": "*",
		"rollup": "^3",
		"vite": ">=3",
		"webpack": "^4 || ^5"
	},
	"peerDependenciesMeta": {
		"@farmfe/core": {
			"optional": true
		},
		"esbuild": {
			"optional": true
		},
		"rollup": {
			"optional": true
		},
		"vite": {
			"optional": true
		},
		"webpack": {
			"optional": true
		}
	},
	"dependencies": {
		"magic-string": "^0.30.17",
		"oxc-parser": "^0.60.0",
		"pathe": "^2.0.3",
		"unplugin": "^2.2.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/node": "^22.13.10",
		"bumpp": "^10.1.0",
		"chalk": "^5.4.1",
		"eslint": "^9.22.0",
		"esno": "^4.8.0",
		"fast-glob": "^3.3.3",
		"nodemon": "^3.1.9",
		"rollup": "^4.36.0",
		"tsup": "^8.4.0",
		"typescript": "^5.8.2",
		"vite": "^6.2.2",
		"vitest": "^3.0.9",
		"webpack": "^5.98.0"
	}
}
